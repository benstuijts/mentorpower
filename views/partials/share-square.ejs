<div class="share-square">
    <a href="http://www.facebook.com/sharer.php?u=<%= url %>" class="share-square-part share-square-facebook">
        <i class="fa fa-facebook-square fa-2x fa-fw"></i>
    </a>
    <a href="http://twitter.com/share?url=<%= url %>" class="share-square-part share-square-twitter">
        <i class="fa fa-twitter fa-2x fa-fw"></i>
    </a>
    <div class="share-square-share">
        S H A R E
    </div>
    
    <a href="https://plus.google.com/share?url=<%= url %>" class="share-square-part share-square-google">
        <i class="fa fa-google fa-2x fa-fw"></i>
    </a>
    
    <a href="https://www.linkedin.com/shareArticle?mini=true&url=<%= url %>" class="share-square-part share-square-linkedin">
        <i class="fa fa-linkedin fa-2x fa-fw"></i>
    </a>
    
    
</div>



